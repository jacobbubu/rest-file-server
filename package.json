{
  "name": "@jacobbubu/rest-file-server",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "main": "dist/index.js",
  "bin": "./bin/index.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "author": "jacobbubu <rong.shen@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jacobbubu/rest-file-server.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "lint": "tslint -t codeFrame --project tsconfig.json 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.build.json",
    "test": "NODE_ENV=test jest --runInBand",
    "test:watch": "NODE_ENV=test jest --watch --runInBand",
    "test:prod": "npm run lint && NODE_ENV=test npm run test --runInBand -- --coverage --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "precommit": "lint-staged",
    "start": "tsnd --respawn src/cli.ts"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/tools/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 75,
        "lines": 78,
        "statements": 78
      }
    },
    "collectCoverage": true
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/concat-stream": "^1.6.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/express-jwt": "0.0.42",
    "@types/jest": "^26.0.13",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.4",
    "@types/node": "^13.11.0",
    "@types/node-getopt": "^0.2.31",
    "@types/rc": "^1.1.0",
    "@types/request": "^2.48.5",
    "@types/shelljs": "^0.8.8",
    "@types/superagent": "^4.1.10",
    "@types/through2": "^2.0.36",
    "axios": "^0.20.0",
    "colors": "^1.4.0",
    "commitizen": "^4.2.1",
    "coveralls": "^3.0.11",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.2.0",
    "enquirer": "^2.3.4",
    "form-data": "^3.0.0",
    "husky": "^4.2.5",
    "is-scoped": "^2.1.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.1.1",
    "lodash.camelcase": "^4.3.0",
    "npm-name": "^6.0.0",
    "prettier": "^2.1.1",
    "prompt": "^1.0.0",
    "replace-in-file": "^5.0.2",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "superagent": "^6.1.0",
    "ts-jest": "^26.3.0",
    "ts-node": "^8.8.1",
    "ts-node-dev": "^1.0.0-pre.62",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^4.0.2",
    "validate-commit-msg": "^2.14.0",
    "validate-npm-package-name": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "concat-stream": "^2.0.0",
    "cors": "^2.8.5",
    "escape-string-regexp": "^4.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "node-getopt": "^0.3.2",
    "through2": "^4.0.2",
    "winston": "^3.3.3"
  }
}
